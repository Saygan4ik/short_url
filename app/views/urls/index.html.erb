<%= render 'links/session_links' %>

<h1>My url's</h1>

<%= render 'errors/flash' %>

<table>
  <tr>
    <th>URL</th>
    <th>Short URL</th>
    <th colspan="1"></th>
  </tr>
  <% @urls.each do |url| %>
  <tr>
    <td><%= url.initial_url %></td>
    <td><%= root_url + url.short_url %></td>
    <td><%= link_to 'Destroy', url_path(url), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
    <% end %>
</table>
<br>
<%= link_to 'Create new', new_url_path %>
